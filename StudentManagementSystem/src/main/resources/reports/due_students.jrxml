	<parameter name="admission" class="java.lang.Integer"/>
	<query language="SQL"><![CDATA[SELECT 

    student.fname, 
    fees.month, 
    fees.fee, 
    fees.paid, 
    fees.due 
FROM 
    fees 
INNER JOIN 
    student 
ON 
    student.admission_no = fees.admission_no 
WHERE 
    fees.due > 0  and fees.admission_no=$P{admission}
    
   
ORDER BY 
    CASE fees.month
        WHEN 'January' THEN 1
        WHEN 'February' THEN 2
        WHEN 'March' THEN 3
        WHEN 'April' THEN 4
        WHEN 'May' THEN 5
        WHEN 'June' THEN 6
        WHEN 'July' THEN 7
        WHEN 'August' THEN 8
        WHEN 'September' THEN 9
        WHEN 'October' THEN 10
        WHEN 'November' THEN 11
        WHEN 'December' THEN 12
        ELSE 13 -- Handles invalid or missing months
    END ASC;
]]></query>
