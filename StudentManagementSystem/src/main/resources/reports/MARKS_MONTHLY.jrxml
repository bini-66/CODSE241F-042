	<parameter name="month" class="java.lang.String"/>
	<query language="SQL"><![CDATA[SELECT marks.admission_no,
       student.fname,
       branch.branchName,
       marks.`Combined Mathematics`,
       marks.Physics,
       marks.Chemistry,
       marks.Total
FROM student
INNER JOIN marks ON student.admission_no = marks.admission_no
INNER JOIN branch ON student.branch_id = branch.branch_id
WHERE marks.`Month` = $P{month}
ORDER BY marks.Total DESC;]]></query>
