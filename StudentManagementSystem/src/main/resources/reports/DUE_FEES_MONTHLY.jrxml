	<parameter name="MONTH" class="java.lang.String"/>
	<query language="SQL"><![CDATA[SELECT 
    fees.admission_no, 
    student.fname, 
    fees.fee, 
    fees.paid, 
    fees.due 
FROM 
    fees 
INNER JOIN 
    student 
ON 
    student.admission_no = fees.admission_no 
WHERE 
    fees.due > 0  and fees.month = $P{MONTH}]]></query>
